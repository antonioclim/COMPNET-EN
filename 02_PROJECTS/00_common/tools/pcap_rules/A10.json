{
  "project": "A10",
  "pcap_relpath": "artifacts/pcap/traffic_e2.pcap",
  "decode_as": [],
  "rules": [
    {
      "id": "R1",
      "filter": "tcp || udp",
      "condition": ">= 10",
      "description": "Container-generated network traffic (to demonstrate egress filtering)."
    },
    {
      "id": "R2",
      "filter": "dns || udp.port==53",
      "condition": ">= 1",
      "description": "Attempted DNS resolution (allowed/blocked)."
    },
    {
      "id": "R3",
      "filter": "tcp.port==80 || tcp.port==443",
      "condition": ">= 1",
      "description": "Attempted external HTTP/HTTPS access (allowed/blocked)."
    }
  ]
}
