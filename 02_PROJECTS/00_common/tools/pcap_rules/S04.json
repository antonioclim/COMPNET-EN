{
  "project": "S04",
  "pcap_relpath": "artifacts/pcap/traffic_e2.pcap",
  "decode_as": [],
  "rules": [
    {
      "id": "R1",
      "filter": "tcp.dstport==3128 && tcp.flags.syn==1 && tcp.flags.ack==0",
      "condition": ">= 1",
      "description": "Client â†’ proxy handshake."
    },
    {
      "id": "R2",
      "filter": "tcp.port==3128 && frame contains \"GET \"",
      "condition": ">= 1",
      "description": "HTTP request to the proxy."
    },
    {
      "id": "R3",
      "filter": "tcp.dstport==8080 && tcp.flags.syn==1 && tcp.flags.ack==0",
      "condition": ">= 1",
      "description": "Proxy initiates a connection to the origin server."
    },
    {
      "id": "R4",
      "filter": "tcp.port==3128 && frame contains \"HTTP/1.1 200\"",
      "condition": ">= 1",
      "description": "Proxy forwards a 200 response to the client."
    }
  ]
}
