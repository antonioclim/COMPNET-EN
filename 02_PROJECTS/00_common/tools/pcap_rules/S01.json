{
  "project": "S01",
  "pcap_relpath": "artifacts/pcap/traffic_e2.pcap",
  "decode_as": [],
  "rules": [
    {
      "id": "R1",
      "filter": "tcp.dstport==5000 && tcp.flags.syn==1 && tcp.flags.ack==0",
      "condition": ">= 1",
      "description": "Connection initiation to the server (SYN)."
    },
    {
      "id": "R2",
      "filter": "tcp.srcport==5000 && tcp.flags.syn==1 && tcp.flags.ack==1",
      "condition": ">= 1",
      "description": "SYNâ€‘ACK response from the server."
    },
    {
      "id": "R3",
      "filter": "tcp.port==5000 && frame contains \"LOGIN\"",
      "condition": ">= 1",
      "description": "A LOGIN command is transmitted (correct framing)."
    },
    {
      "id": "R4",
      "filter": "tcp.port==5000 && frame contains \"MSG \"",
      "condition": ">= 1",
      "description": "At least one MSG message is transmitted."
    },
    {
      "id": "R5",
      "filter": "tcp.port==5000 && frame contains \"EVENT \"",
      "condition": ">= 1",
      "description": "The server sends at least one push event (presence)."
    },
    {
      "id": "R6",
      "filter": "tcp.port==5000 && (tcp.flags.fin==1 || tcp.flags.reset==1)",
      "condition": ">= 1",
      "description": "Session closure (FIN or documented RST)."
    }
  ]
}
