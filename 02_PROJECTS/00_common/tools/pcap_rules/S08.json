{
  "project": "S08",
  "pcap_relpath": "artifacts/pcap/traffic_e2.pcap",
  "decode_as": [],
  "rules": [
    {
      "id": "R1",
      "filter": "tcp.dstport==2525 && tcp.flags.syn==1 && tcp.flags.ack==0",
      "condition": ">= 1",
      "description": "Client â†’ SMTP handshake."
    },
    {
      "id": "R2",
      "filter": "tcp.port==2525 && (frame contains \"EHLO\" || frame contains \"HELO\")",
      "condition": ">= 1",
      "description": "SMTP greeting (EHLO/HELO)."
    },
    {
      "id": "R3",
      "filter": "tcp.port==2525 && frame contains \"MAIL FROM\"",
      "condition": ">= 1",
      "description": "MAIL FROM present."
    },
    {
      "id": "R4",
      "filter": "tcp.port==2525 && frame contains \"RCPT TO\"",
      "condition": ">= 1",
      "description": "RCPT TO present."
    },
    {
      "id": "R5",
      "filter": "tcp.port==2525 && frame contains \"DATA\"",
      "condition": ">= 1",
      "description": "DATA command present."
    },
    {
      "id": "R6",
      "filter": "tcp.port==2525 && (frame contains \"\\r\\n.\\r\\n\" || frame contains \"\\n.\\n\")",
      "condition": ">= 1",
      "description": "The data terminator (dot) appears in the payload (dot-stuffing is demonstrable)."
    }
  ]
}
